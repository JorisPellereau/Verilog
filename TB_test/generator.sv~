 * 
 */
 
 
 class generator;

    // Declaring transaction class
    rand transaction trans;


    // Main Task
    task main();
       trans = new();
       if(!trans.randomize() ) $fatal("Gen:: trans randomization failed");
       gen2driv.put(trans);
       
    endtask // main
    
    
 endclass // generator

 
